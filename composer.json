{
    "name": "onurlib/fusion-auth-integration-bundle",
    "namespace": "OnurLib\\FusionAuthIntegrationBundle",
    "type": "symfony-bundle",
    "description": "Integration bundle for FusionAuth with Symfony",
    "keywords": ["symfony", "fusionauth", "bundle"],
    "license": "MIT",
    "authors": [
        {
            "name": "Onur Kudret",
            "email": "onurkudrt31@gmail.com"
        }
    ],
    "version": "1.0.0",
    "require": {
        "php": "^8.0",
        "symfony/framework-bundle": "^7.0",
        "symfony/dependency-injection": "^7.0",
        "symfony/yaml": "^7.1",
        "symfony/cache": "^6.0",
        "symfony/http-kernel": "^7.0",
        "symfony/console": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "FusionAuthIntegrationBundle\\": "src/FusionAuthIntegrationBundle"
        }
    }
    ,
    "autoload-dev": {
        "psr-4": {
            "FusionAuthIntegrationBundle\\Tests\\": "tests/"
        }
    }
}
